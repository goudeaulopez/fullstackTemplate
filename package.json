{
  "name": "fullstacktemplate",
  "version": "1.0.0",
  "description": "it is a simple template for express and react",
  "main": "index.js",
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goudeaulopez/fullstackTemplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goudeaulopez/fullstackTemplate/issues"
  },
  "homepage": "https://github.com/goudeaulopez/fullstackTemplate#readme",
  "dependencies": {
    "concurrently": "^7.2.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.16"
  }
}
